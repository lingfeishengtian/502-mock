<div class="flex justify-between items-center mb-4">
    <h1>Previous Guest Speakers</h1>
    <% if current_member.admin? %>
        <%= link_to 'Add Speaker', new_speaker_path, class: 'bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded' %>
    <% end %>
</div>

<ul>
        <% @speakers.each do |speaker| %>
                <%= render partial: 'speaker', locals: { speaker: speaker } %>
        <% end %>
</ul>